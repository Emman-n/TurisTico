             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TurisTico.View.API">

    <StackLayout>


        <SearchBar SearchCommand="{Binding buscarCommand}"
                   Placeholder="Buscar por cordenadas..."/>

