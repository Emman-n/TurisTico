<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="TurisTico.MainPage"
            xmlns:local1="clr-namespace:TurisTico.View"
            Title="Menu">

    <FlyoutPage.Flyout>
        <local1:FlyoutMenuPage x:Name="flyout"/>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local1:Inicio/>
            </x:Arguments>
        </NavigationPage>

    </FlyoutPage.Detail>
    
</FlyoutPage>
